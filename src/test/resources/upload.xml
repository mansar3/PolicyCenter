<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Selenium Grid - POS2 Regression Test" configfailurepolicy="continue" parallel="classes" data-provider-thread-count="25">
	<parameter name="local" value="true" />
	<parameter name="sharedFolder" value="true" />


	<test name="Upload XML" >
		<parameter name="userName" value="su" />
		<parameter name="passWord" value="su" />
		<parameter name="sendEmail" value="false"/>
		<classes>
			<class name="PolicyConversion.UploadXML" >
				<methods>
					<include name="uploadXML" ></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>